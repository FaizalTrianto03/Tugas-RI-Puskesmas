# ───────────────────────────────────────────────────────────────
# Firebase Configuration
# ───────────────────────────────────────────────────────────────
# Copy this file to .env and fill with your Firebase credentials
# NEVER commit .env file to version control

# ─── Android ───
FIREBASE_ANDROID_API_KEY=your_android_api_key_here
FIREBASE_ANDROID_APP_ID=your_android_app_id_here
FIREBASE_ANDROID_MESSAGING_SENDER_ID=your_messaging_sender_id_here
FIREBASE_ANDROID_PROJECT_ID=your_project_id_here
FIREBASE_ANDROID_STORAGE_BUCKET=your_storage_bucket_here

# ─── iOS ───
FIREBASE_IOS_API_KEY=your_ios_api_key_here
FIREBASE_IOS_APP_ID=your_ios_app_id_here
FIREBASE_IOS_MESSAGING_SENDER_ID=your_messaging_sender_id_here
FIREBASE_IOS_PROJECT_ID=your_project_id_here
FIREBASE_IOS_STORAGE_BUCKET=your_storage_bucket_here
FIREBASE_IOS_BUNDLE_ID=com.example.puskesmasapp

# ─── macOS ───
FIREBASE_MACOS_API_KEY=your_macos_api_key_here
FIREBASE_MACOS_APP_ID=your_macos_app_id_here
FIREBASE_MACOS_MESSAGING_SENDER_ID=your_messaging_sender_id_here
FIREBASE_MACOS_PROJECT_ID=your_project_id_here
FIREBASE_MACOS_STORAGE_BUCKET=your_storage_bucket_here
FIREBASE_MACOS_BUNDLE_ID=com.example.puskesmasapp

# ─── Web ───
FIREBASE_WEB_API_KEY=your_web_api_key_here
FIREBASE_WEB_APP_ID=your_web_app_id_here
FIREBASE_WEB_MESSAGING_SENDER_ID=your_messaging_sender_id_here
FIREBASE_WEB_PROJECT_ID=your_project_id_here
FIREBASE_WEB_AUTH_DOMAIN=your_auth_domain_here
FIREBASE_WEB_STORAGE_BUCKET=your_storage_bucket_here

# ─── Windows ───
FIREBASE_WINDOWS_API_KEY=your_windows_api_key_here
FIREBASE_WINDOWS_APP_ID=your_windows_app_id_here
FIREBASE_WINDOWS_MESSAGING_SENDER_ID=your_messaging_sender_id_here
FIREBASE_WINDOWS_PROJECT_ID=your_project_id_here
FIREBASE_WINDOWS_AUTH_DOMAIN=your_auth_domain_here
FIREBASE_WINDOWS_STORAGE_BUCKET=your_storage_bucket_here
